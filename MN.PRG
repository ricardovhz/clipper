FUNCTION MN(LI,CI,COR,NQTD,TEXT1,TEXT2,TEXT3,TEXT4,TEXT5,TEXT6)
   NMENU := 1
   T1 := TEXT1
   T2 := TEXT2
   T3 := TEXT3
   T4 := TEXT4
   T5 := TEXT5
   T6 := TEXT6
   FOR I = 1 TO VAL( NQTD )
      TOL := "T"+SUBS( "123456",I,1 )
      CLI := VAL( LI )
      CCI := VAL( CI )
      SETCOLOR(COR)
      @ CLI-1+I,CCI PROMPT &TOL
   NEXT
   MENU TO NMENU
   RETURN NMENU
