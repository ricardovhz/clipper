#INCLUDE "INKEY.CH"
LEI := SPACE(10)
CLS
SETCOLOR("N/W")
@ 24,00 SAY PADC("",79)
@ 24,00 SAY PADR("Insert",79)
SETCOLOR("W/N")
SET KEY K_INS TO T_INS
READINSERT(.T.)
MEMOEDIT( LEI,00,00,23,79,.T. )
SET KEY K_INS TO
CLS
QUIT

**************
FUNCTION T_INS
**************
SAVELIN := ROW()
SAVECOL := COL()
SETCOLOR("N/W")
@ 24,00 SAY IIF( READINSERT(),PADR( "Insert",79 ),PADR( "      ",79 ) )
SETCOLOR("W/N")
SETPOS( SAVELIN,SAVECOL )
RETURN (0)
